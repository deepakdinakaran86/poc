package com.pcs.avocado.commons.exception;

/**
 * GalaxyErrorCode
 * 
 * @description Interface for ErroCodes in Galaxy,any Error Code Enum should
 *              implement this class
 * @author Javid Ahammed (PCSEG199)
 * @date 04 November 2014
 * @since galaxy-1.0.0
 */
public interface GalaxyErrorCode {

	/**
	 * Interface method to be implemented to get the Error Code
	 * 
	 * @return
	 */
	public Integer getCode();

	/**
	 * Interface method to be implemented to get the Error Message
	 * 
	 * @return
	 */
	public String getMessage();

	/**
	 * Interface method to be implemented to get the Error Description which
	 * must be helpful for debuggin
	 * 
	 * @return
	 */
	public String getDescription();

}
