<?xml version="1.0" encoding="UTF-8"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  <groupId>skyspark-esb</groupId>
  <artifactId>galaxy-skyspark-registry-resource-stagging-composite-app</artifactId>
  <version>1.0.0</version>
  <packaging>carbon/application</packaging>
  <name>galaxy-skyspark-registry-resource-stagging-composite-app</name>
  <description>galaxy-skyspark-registry-resource-stagging-composite-app</description>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-eclipse-plugin</artifactId>
        <version>2.9</version>
        <configuration>
          <buildcommands />
          <projectnatures>
            <projectnature>org.wso2.developerstudio.eclipse.distribution.project.nature</projectnature>
          </projectnatures>
        </configuration>
      </plugin>
      <plugin>
        <groupId>org.wso2.maven</groupId>
        <artifactId>maven-car-plugin</artifactId>
        <version>2.0.9</version>
        <extensions>true</extensions>
        <executions>
          <execution>
            <id>car</id>
            <phase>package</phase>
            <goals>
              <goal>car</goal>
            </goals>
          </execution>
        </executions>
        <configuration />
      </plugin>
      <plugin>
        <groupId>org.wso2.maven</groupId>
        <artifactId>maven-car-deploy-plugin</artifactId>
        <version>1.0.5</version>
        <extensions>true</extensions>
        <configuration>
          <carbonServers>
            <CarbonServer>
              <trustStorePath>${server.trustStorePath}</trustStorePath>
              <trustStorePassword>${server.trustStorePassword}</trustStorePassword>
              <trustStoreType>JKS</trustStoreType>
              <serverUrl>${server.url}</serverUrl>
              <userName>${server.username}</userName>
              <password>${server.password}</password>
              <operation>deploy</operation>
            </CarbonServer>
          </carbonServers>
        </configuration>
      </plugin>
    </plugins>
  </build>
  <profiles>
    <profile>
      <id>development</id>
      <properties>
        <server.trustStorePassword>wso2carbon</server.trustStorePassword>
        <server.username>admin</server.username>
        <server.trustStorePath>E:/KeyStore/wso2carbon.jks</server.trustStorePath>
        <server.password>devadmin</server.password>
        <server.url>https://10.234.31.202:9445</server.url>
      </properties>
    </profile>
  </profiles>
  <repositories>
    <repository>
      <releases>
        <updatePolicy>daily</updatePolicy>
        <checksumPolicy>ignore</checksumPolicy>
      </releases>
      <id>wso2-nexus</id>
      <url>http://maven.wso2.org/nexus/content/groups/wso2-public/</url>
    </repository>
    <repository>
      <id>wso2-maven2-repository-1</id>
      <url>http://dist.wso2.org/maven2</url>
    </repository>
    <repository>
      <id>wso2-nexus-repository-1</id>
