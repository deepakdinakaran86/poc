.modal-header {
	min-height: 16.43px;
	padding: 15px;
	border-bottom: 1px solid #e5e5e5;
	background-color: #00A8E7;
}

.modal-footer {
	padding: 10px;
	text-align: right;
	border-top: 1px solid #e5e5e5;
	background-color: #eaeaea;
}

.modal-title {
	color: #FFFFFF;
	margin-top: 0px;
	font-size: 16px;
	margin-bottom: 0px;
	font-weight: 600;
}

.modal-body {
	font-size: 14px;
	height: 200px;
	overflow: auto;
}

thead {
	color: #66429A
}

.gx-action-history-css {
	font-size: 14px;
	min-width: 860px;
}

.gx-action-history-css th {
	padding-right: 0px;
}

.gx-action-history-css td {
	padding-right: 0px;
}

.btn-default {
	color: #FFFFFF;
	background-color: #00A8E7;
	border-color: #ebebeb;
}

.btn-default:hover {
	color: #FFF;
	background-color: #93268f;
}