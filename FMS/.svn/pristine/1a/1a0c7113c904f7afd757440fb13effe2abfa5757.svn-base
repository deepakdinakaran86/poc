	.btn-boxapp {
    border-radius: 3px;
    position: relative;
    padding: 15px 5px;
    margin: 0 0 10px 10px;
    width: 81px;
    height: 91px;
    text-align: center;
    color: #fff;
    border: 1px solid #ddd;
    background-color: #f4f4f4;
	background-color:#357ca5;
	/*background-color: #00c0ef !important;*/
    font-size: 12px;
}

.btn-boxapp:hover{
	background-color:##73bde8;
}

.count.text-blue {
    color: #fff !important;
    background: #393939;
    height: 24px;
    width: 50px;
    display: block;
    /* border-radius: 81%; */
    border-radius: 48px;
    margin: 0 auto;
    top: 2px;
    padding: 2px 0 0 0;
}
.btn-boxapp .count{font-size:12px;}
.btn-boxapp .fa{font-size:20px;padding:8px 0px}
.btn-boxapp h5{    margin-top: 5px;
   margin-top: 12px;
    font-weight: bold;
    text-transform: uppercase;
    margin-bottom: 5px;
	}

.btn-boxapp figure{
    display: inline-block;
    width: inherit;
    height: 32px;
    background-repeat: no-repeat;
    background-position: top center;
    margin-left: -8px;
	margin-bottom: 7px;
}	
