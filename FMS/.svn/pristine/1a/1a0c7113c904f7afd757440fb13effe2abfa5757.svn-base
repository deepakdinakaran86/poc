/*******************************/
/******** Button Styles ********/
/*******************************/




 .btnanimation li.primary{
	    border: 1px solid #00c0ef;
    background-color: #00c0ef;
  }
  
  .btnanimation li.primary i{
  color: #fff;
     position: relative;
    top: 1px;
  }
  
   .btnanimation li.default{
      border: 1px solid #bdbbbb;
    background-color: #eceaea;
	}
	
	 .btnanimation li.default i{
  color: #000;
     position: relative;
    top: 1px;
  }
  
    .btnanimation li.edit{
	border: 1px solid #d73925;
	background-color: #d73925;
  }
  
  .btnanimation li.edit i{
  color: #fff;
      position: relative;
    top: 1px;
  }
  
  
 .btnanimation li {
    overflow: hidden;
    background: #ddd;
    list-style: none;
    white-space:nowrap;
    margin-right: 6px;
    border-radius: 3px;
    display: inline-block;
	padding: 7px;
}
    .btnanimation li button	 {
        opacity: 0;
        color: #666;
        max-width: 0;
        display: inline-block;
        text-decoration: none;
        transition: max-width 1s ease-out .1s, opacity 1s ease-out .1s, color;
        background: none;
    	border: none;
    	padding:0;
    }
        .btnanimation li button:hover {
            color: #c33;
        }
        .btnanimation li button i {
            float:right;
            display: block;
            padding-right: 1em;
            font:1em/1 'rokkitt', sans-serif;
			    padding-left: 10px;
        }
    .btnanimation li b {
        color: #c33;
        line-height:1;
        font-size: 25px;
        display: inline-block;
        padding:.125em .375em;
    }
    .btnanimation li:hover {}
        .btnanimation li:hover button {
            opacity: 1;
            max-width: 150px;
            transition: max-width 1s ease-out .1s, opacity 1s ease-out .1s, color .2s;
        }


