package com.pcs.alpine.commons.sms.dto;

import java.io.Serializable;

import javax.xml.bind.annotation.XmlRootElement;

/**
 * SMSDTO
 * 
 * This class is responsible for identifying parameters for sending sms
 * 
 * @author Deepak Dinakaran (PCSEG288)
 * @date 8 May 2016
 * @since alpine-1.0.0
 */

@XmlRootElement
public class SMSStatsDTO implements Serializable {

	private static final long serialVersionUID = 7334892184583579077L;

	private String accountId;
	
	private String from;
	
	private String to;
	
	private String date;
	
	private String status;
	
	private String price;
	
	private String priceUnit;
	
	private String errorMessage;
	
	private String direction;
	
	private String body;
	
	public String getAccountId() {
		return accountId;
	}

	public void setAccountId(String accountId) {
		this.accountId = accountId;
	}

	public String getFrom() {
		return from;
	}

	public void setFrom(String from) {
		this.from = from;
	}

	public String getTo() {
		return to;
	}

	public void setTo(String to) {
		this.to = to;
	}

	public String getDate() {
		return date;
	}

	public void setDate(String date) {
		this.date = date;
	}

	public String getStatus() {
		return status;
	}

	public void setStatus(String status) {
		this.status = status;
	}

	public String getPrice() {
		return price;
	}

	public void setPrice(String price) {
		this.price = price;
	}

	public String getPriceUnit() {
		return priceUnit;
	}

	public void setPriceUnit(String priceUnit) {
		this.priceUnit = priceUnit;
	}

	public String getErrorMessage() {
		return errorMessage;
	}

	public void setErrorMessage(String errorMessage) {
		this.errorMessage = errorMessage;
	}

	public String getDirection() {
		return direction;
	}

	public void setDirection(String direction) {
		this.direction = direction;
	}

	public String getBody() {
		return body;
	}

	public void setBody(String body) {
		this.body = body;
	}
	
	
	
}
