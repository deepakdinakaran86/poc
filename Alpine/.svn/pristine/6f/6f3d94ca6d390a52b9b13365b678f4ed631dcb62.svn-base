package com.pcs.alpine.commons.rest.dto;

/**
 * ErrorMessageISDTO
 * 
 * @description DTO which Comprises error information that is required to send
 *              back to the clients.
 * @author Deepak Dinakaran PCSEG288
 * @date 12 May 2016
 * @since alpine-1.0.0
 */
public class ErrorMessageISDTO extends ErrorMessageDTO {
	
	private String status;
	
	private String message;
	
	public String getStatus() {
		return status;
	}
	public void setStatus(String status) {
		this.status = status;
	}
	public String getMessage() {
		return message;
	}
	public void setMessage(String message) {
		this.message = message;
	}
	

}
