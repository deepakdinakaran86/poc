package com.pcs.alpine.dto;

import com.pcs.alpine.services.dto.EntityDTO;
import com.pcs.alpine.services.dto.IdentityDTO;

/**
 * 
 * DTO used for user services
 * 
 * @author Javid Ahammed (pcseg199)
 * @date January 2016
 * @since avocado-1.0.0
 */
public class ESBUserDTO {

	private EntityDTO userEntity;
	
	private LinkEmailESBDTO linkEmailESBDTO;
	
	private IdentityDTO identityDTO;
	
	private UserDTO userDTO;

	public EntityDTO getUserEntity() {
		return userEntity;
	}

	public void setUserEntity(EntityDTO userEntity) {
		this.userEntity = userEntity;
	}

	public LinkEmailESBDTO getLinkEmailESBDTO() {
		return linkEmailESBDTO;
	}

	public void setLinkEmailESBDTO(LinkEmailESBDTO linkEmailESBDTO) {
		this.linkEmailESBDTO = linkEmailESBDTO;
	}

	public IdentityDTO getIdentityDTO() {
		return identityDTO;
	}

	public void setIdentityDTO(IdentityDTO identityDTO) {
		this.identityDTO = identityDTO;
	}

	public UserDTO getUserDTO() {
		return userDTO;
	}

	public void setUserDTO(UserDTO userDTO) {
		this.userDTO = userDTO;
	}

}
