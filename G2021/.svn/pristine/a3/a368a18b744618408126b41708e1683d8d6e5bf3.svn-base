					$('#asset-alarms-toggle').hide();
					
				}else{
					//alert('not visible');
					//$(".form-basic input").css('width','250px');
					$(".form-basic input").animate({width:150},250);
					$(".form-basic .form-row > label span").animate({width:250},475);
					$("#device-unit").css('width','35px');
					$("#genset-grid-toggle").toggleClass("col-md-5 col-md-8");
					$('#alarm-bell').hide();
					$('#asset-alarms-toggle').show();
				
				}

				return false;
			});
		
		});
		
	$("#menu-toggle").click(function(e) {
	e.preventDefault();
	$("#wrapper").toggleClass("toggled");
	});
     $("#menu-toggle-2").click(function(e) {
		e.preventDefault();
		$("#wrapper").toggleClass("toggled-2");
		$('#menu ul').hide();
		$('#treeview').toggle();
		
		if($( ".caption-title" ).hasClass( "rotate" )){
			$( ".sidebar-title .caption-title" ).removeClass( "rotate" );
		}else{
			$( ".sidebar-title .caption-title" ).addClass( "rotate" );
		}
		
		if($( ".sidebar-title .fa" ).hasClass( "fa-angle-double-right" )){
			$( ".sidebar-title .fa" ).removeClass( "fa-angle-double-right" );
			$( ".sidebar-title .fa" ).addClass( "fa-angle-double-down" );
		}else{
			$( ".sidebar-title .fa" ).removeClass( "fa-angle-double-down" );
			$( ".sidebar-title .fa" ).addClass( "fa-angle-double-right" );
		}
		
		
    });

     function initMenu() {
      $('#menu ul').hide();
      $('#menu ul').children('.current').parent().show();
      //$('#menu ul:first').show();
      $('#menu li a').click(
        function() {
          var checkElement = $(this).next();
          if((checkElement.is('ul')) && (checkElement.is(':visible'))) {
            return false;
            }
          if((checkElement.is('ul')) && (!checkElement.is(':visible'))) {
            $('#menu ul:visible').slideUp('normal');
            checkElement.slideDown('normal');
            return false;
            }
          }
        );
      }
    $(document).ready(function() {/*initMenu();*/});

	</script>
	
