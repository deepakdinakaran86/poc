.menu, .menu-bg, .menu-burger {
	position: absolute;
    width: 50px;
    height: 50px;
    font-size: 30px;
    text-align: center;
    border-radius:0;
    right: 25px;
    top: 25px;
    bottom: 0;
}

.menu-bg {
  background: #28303b;
  pointer-events: none;
  transition: .3s;
  right: 50px;
  top: 50px;
  transform: translate3d(50%, -50%, 0);
  transform-origin: center center;
  opacity:0.9
}
.menu-bg.fs {
  transform: translate3d(50%, -50%, 0);
  width: 300vw;
  height: 300px;
}

.menu-burger {
  color: white;
padding-top: 4px;
    -webkit-user-select: none;
    cursor: pointer;
    transition: .4s;
    transform-origin: center;
}
.menu-burger.fs {
  transform: rotate(-180deg) translateX(10px);
}

.menu-items {
  position: absolute;
  font-weight: bold;
  font-size: 40px;
  letter-spacing: 10px;
  color: white;
  width: 100%;
  text-align: center;
  opacity: 0;
  transition: .4s;
  /*margin-top: 100px;*/
  transform: translateY(-200%);
  pointer-events: none;
}
.menu-items div {
  transition: 1s;
  opacity: 0;
  margin-top: 0px;
}


.menu-items.fs {
  transform: translateY(0);
  pointer-events: auto;
  opacity: 1;
}
.menu-items.fs div {
  opacity: 1;
  margin-top: 5px;
}

