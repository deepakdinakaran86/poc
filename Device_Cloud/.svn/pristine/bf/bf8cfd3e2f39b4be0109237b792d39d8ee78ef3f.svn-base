package com.pcs.datasource.enums;

public enum ResponseMessage {

	SI_UNIT_EXISTS("SI Unit already exists"), CONVERSION_NOT_SPECIFIED(
	        "Conversion not specified"), UNIT_EXISTS("Unit already exists"),
	NAME_NOT_SPECIFIED("Name not specified"), ISSI_NOT_SPECIFIED(
	        "Is SI Flag not specified"), DUPLICATES_FOUND(
	        "Duplicate records found"),
	NO_SI_SPECIFIED("No SI unit specified"), ;

	private String Message;

	private ResponseMessage(String message) {
		Message = message;
	}

	public String getMessage() {
		return Message;
	}

	public void setMessage(String message) {
		Message = message;
	}

}
