package com.pcs.saffron.g2021.SimulatorEngine.CS.message;

import com.pcs.saffron.g2021.SimulatorEngine.CS.message.header.Header;

public class ServerMessage extends Header{
			
	private static final long serialVersionUID = 1L;
	
	private Short bufferSize;
	private Object payload;
	
	public Short getBufferSize() {
		return bufferSize;
	}
	public void setBufferSize(Short bufferSize) {
		this.bufferSize = bufferSize;
	}
	
	public Object getPayload() {
		return payload;
	}
	public void setPayload(Object payload) {
		this.payload = payload;
	}
	
}
