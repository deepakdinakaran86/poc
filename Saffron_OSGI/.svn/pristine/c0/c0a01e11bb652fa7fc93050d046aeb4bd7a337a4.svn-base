
package com.pcs.saffron.connectivity.mqtt;

/**
 * This exception is raised when there is no active device connected which is
 * represented by a Destination object.
 * 
 *  @author renukaradhya
 */
public class NoDeviceConnectedException extends Exception {
    
    /**
	 * 
	 */
	private static final long serialVersionUID = -3234734674592677399L;

	public NoDeviceConnectedException(final String msg) {
        super(msg);
    }

}
