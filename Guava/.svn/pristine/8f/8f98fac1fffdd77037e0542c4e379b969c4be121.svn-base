package com.pcs.guava.dto.scheduling;

import java.util.List;

import com.pcs.guava.commons.dto.EntityDTO;
import com.pcs.guava.commons.dto.IdentityDTO;


/**
 * RoutingESBDTO
 *
 * @description DTO for scheduling esb
 * @author Sekh (PCSEG336)
 * @date 22 Jun 2016
 * @since galaxy-1.0.0
 */
public class SchedulingESBDTO {
	
	private IdentityDTO routeIdentity;
	
	private EntityDTO scheduleEntity;
	
	private List<EntityDTO> scheduleDestination;
	
	public EntityDTO getScheduleEntity() {
		return scheduleEntity;
	}
	public void setScheduleEntity(EntityDTO scheduleEntity) {
		this.scheduleEntity = scheduleEntity;
	}
	public List<EntityDTO> getScheduleDestination() {
		return scheduleDestination;
	}
	public void setScheduleDestination(List<EntityDTO> scheduleDestination) {
		this.scheduleDestination = scheduleDestination;
	}
	public IdentityDTO getRouteIdentity() {
		return routeIdentity;
	}
	public void setRouteIdentity(IdentityDTO routeIdentity) {
		this.routeIdentity = routeIdentity;
	}

}
