package com.pcs.guava.enums;

/**
 * Status
 * 
 * @description Holds the Status information
 * @author Anish Prabhakaran (PCSEG271)
 * @date 24th Aug 2014
 * @since galaxy-1.0.0
 */
public enum Status {

	ACTIVE("active"), INACTIVE("inactive"), DELETED("deleted"), SUCCESS(
			"success"), FAILURE("failure"), PENDING("PENDING"), PARTIAL(
			"Partial"), QUEUED("QUEUED"), TRUE("true"), ALLOCATED("ALLOCATED"), ALL(
			"all");

	private String status;

	Status(String status) {
		this.status = status;
	}

	public String getStatus() {
		return status;
	}

	public void setStatus(String status) {
		this.status = status;
	}
}
